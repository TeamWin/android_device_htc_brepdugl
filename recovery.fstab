# mount point		fstype  device					device2			flags
/system			ext4	/dev/block/bootdevice/by-name/system
/system_image		emmc	/dev/block/bootdevice/by-name/system				flags=flashimg=1
/vendor			ext4	/dev/block/bootdevice/by-name/vendor				flags=backup=1;mounttodecrypt
/vendor_image		emmc	/dev/block/bootdevice/by-name/vendor				flags=backup=1;flashimg=1
/data			ext4	/dev/block/bootdevice/by-name/userdata				flags=length=-163840;encryptable=footer
/boot			emmc	/dev/block/bootdevice/by-name/boot
/recovery		emmc	/dev/block/bootdevice/by-name/recovery
/misc			emmc	/dev/block/bootdevice/by-name/misc
/cota			ext4	/dev/block/bootdevice/by-name/cota

# Removable storage
/external_sd		vfat	/dev/block/mmcblk0p1			/dev/block/mmcblk0	flags=storage;wipeingui;removable;encryptable=footer
/usb_otg		vfat	/dev/block/sda1				/dev/block/sda		flags=storage;wipeingui;removable
